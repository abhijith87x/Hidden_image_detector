import tensorflow as tf
import numpy as np
import cv2
model = tf.keras.models.load_model("model\hidden_image_detector.h5")
IMG_SIZE = 128

def predict_image(image_path):
    img = cv2.imread(r"D:\random_images\t3_l5f9bw.jpg")
    img = cv2.resize(img, (IMG_SIZE, IMG_SIZE))
    img = img / 255.0
    img = np.expand_dims(img, axis=0)
    prediction = model.predict(img)[0][0]
    
    if prediction > 0.5:
        print("NO Hidden Image Detected")
    else:
        print("Hidden Image Detected")

predict_image("test.jpg")